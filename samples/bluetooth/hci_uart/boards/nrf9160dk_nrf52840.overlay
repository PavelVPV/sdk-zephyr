#include <nrf9160dk_nrf52840_reset_on_if9.dtsi>
#include <nrf9160dk_uart1_on_if0_3.dtsi>

&uart1 {
	current-speed = <1000000>;
	hw-flow-control;
};

&gpiote {
	interrupts = <6 NRF_DEFAULT_IRQ_PRIORITY>;
};

/ {
	chosen {
		zephyr,bt-c2h-uart=&uart1;
	};
};
